{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "api/health.js",
      "methods": ["GET"],
      "headers": {
        "Cache-Control": "no-store, max-age=0"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  }
  {

}
}
